#range(start,stop,stop) 
nums=[1,2,3,4]
for i in range (5): #for loop
    print (i)
for i in range  (0,5):
    print (i)
for i in reversed(nums):
    print (i)
for i in range (0,5,2):
    print (i)
    #list method 
print (range(5))
print (range(0,5))
print (list(range(0,5,2)))

